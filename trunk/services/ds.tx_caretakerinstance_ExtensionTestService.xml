<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms> 
			<sheetTitle>Default</sheetTitle> 
		</TCEforms> 
		<type>array</type>
		<el>
			<extension_key>
				<TCEforms>
					<label>Extension Key</label>
					<config>
						<type>input</type>
						<size>30</size>
					</config>
				</TCEforms>
			</extension_key>
			<requirement_mode>
				<TCEforms>
					<label>Requirement Mode</label>
					<config>
						<type>radio</type>
						<items>
							<numIndex index='0'>
								<numIndex index="0">none</numIndex>
								<numIndex index="1">none</numIndex>
							</numIndex>
							<numIndex index='1'>
								<numIndex index="0">required</numIndex>
								<numIndex index="1">required</numIndex>
							</numIndex>
							<numIndex index='2'>
								<numIndex index="0">forbidden</numIndex>
								<numIndex index="1">forbidden</numIndex>
							</numIndex>
						</items>
						<default>0</default>
					</config>
				</TCEforms>
			</requirement_mode>
			<min_version>
				<TCEforms>
					<label>Minimal allowed Version</label>
					<config>
						<type>input</type>
						<size>30</size>
					</config>
				</TCEforms>
			</min_version>
			<max_version>
				<TCEforms>
					<label>Maximal allowed Version</label>
					<config>
						<type>input</type>
						<size>30</size>
					</config>
				</TCEforms>
			</max_version>
		</el>
	</ROOT>
</T3DataStructure>