<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms> 
			<sheetTitle>Default</sheetTitle> 
		</TCEforms> 
		<type>array</type>
		<el>
			<checkConfVars>
				<TCEforms>
					<label>Check TYPO3_CONF_VARS : key_pipe_subkey... = expectation OR key_pipe_subkey ... :regex: regular expression</label>
					<config>
						<type>text</type>
						<cols>80</cols>
						<rows>3</rows>
					</config>
				</TCEforms>
			</checkConfVars>
			<resultMatch>
				<TCEforms>
					<label>Result if conditions match</label>
					<config>
						<type>select</type>
						<items>
							<numIndex index="0">
					    		<numIndex index="0">OK</numIndex>
								<numIndex index="1">1</numIndex>
					    	</numIndex>
					    	<numIndex index="1">
					    		<numIndex index="0">Warning</numIndex>
								<numIndex index="1">0</numIndex>
					    	</numIndex>
					    	<numIndex index="2">
					    		<numIndex index="0">Error</numIndex>
								<numIndex index="1">1</numIndex>
					    	</numIndex>
						</items>
						<default>2</default>
					</config>
				</TCEforms>
			</resultMatch>
			<resultNoMatch>
				<TCEforms>
					<label>Result ic condition do'nt match:</label>
					<config>
						<type>select</type>
						<items>
							<numIndex index="0">
					    		<numIndex index="0">OK</numIndex>
								<numIndex index="1">1</numIndex>
					    	</numIndex>
					    	<numIndex index="1">
					    		<numIndex index="0">Warning</numIndex>
								<numIndex index="1">0</numIndex>
					    	</numIndex>
					    	<numIndex index="2">
					    		<numIndex index="0">Error</numIndex>
								<numIndex index="1">1</numIndex>
					    	</numIndex>
						</items>
						<default>0</default>
					</config>
				</TCEforms>
			</resultNoMatch>
		</el>
	</ROOT>
</T3DataStructure>