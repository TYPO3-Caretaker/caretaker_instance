<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <ROOT>
        <TCEforms>
            <sheetTitle>Default</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <table>
                <TCEforms>
                    <label>Table name</label>
                    <config>
                        <type>input</type>
                        <size>30</size>
                        <eval>required,trim</eval>
                    </config>
                </TCEforms>
            </table>
            <field>
                <TCEforms>
                    <label>Table field</label>
                    <config>
                        <type>input</type>
                        <size>30</size>
                        <eval>required,trim</eval>
                    </config>
                </TCEforms>
            </field>
            <blacklist>
                <TCEforms>
                    <label>Blacklisted values</label>
                    <config>
                        <type>text</type>
                        <eval>required</eval>
                    </config>
                </TCEforms>
            </blacklist>
        </el>
    </ROOT>
</T3DataStructure>
