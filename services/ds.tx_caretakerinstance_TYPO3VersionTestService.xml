<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms> 
			<sheetTitle>Default</sheetTitle> 
		</TCEforms> 
		<type>array</type>
		<el>
			<min_version>
				<TCEforms>
					<label>Minimal allowed Version (e.g. '4.3.2' or '4.3.latest')</label>
					<config>
						<type>input</type>
						<size>30</size>
					</config>
				</TCEforms>
			</min_version>
			<max_version>
				<TCEforms>
					<label>Maximal allowed Version (e.g. '4.3.2' or '4.3.latest')</label>
					<config>
						<type>input</type>
						<size>30</size>
					</config>
				</TCEforms>
			</max_version>
			<dont_allow_dev_versions>
				<TCEforms>
					<label>Don't allow latest to be developement versions (alpha,beta,rc etc.)</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</dont_allow_dev_versions>
		</el>
	</ROOT>
</T3DataStructure>