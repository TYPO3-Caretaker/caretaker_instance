<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <ROOT>
        <TCEforms>
            <sheetTitle>Default</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            <min_version>
                <TCEforms>
					<label>Minimal allowed Version (e.g. '4.3.2' or '4.3.latest' or '4.3.secure')</label>
                    <config>
                        <type>input</type>
                        <size>30</size>
                    </config>
                </TCEforms>
            </min_version>
            <max_version>
                <TCEforms>
					<label>Maximal allowed Version (e.g. '4.3.2' or '4.3.latest' or '4.3.secure')</label>
                    <config>
                        <type>input</type>
                        <size>30</size>
                    </config>
                </TCEforms>
            </max_version>
            <allow_unstable>
                <TCEforms>
                    <label>Allow unstable releases (e.G. release candidate versions)</label>
                    <config>
                        <type>check</type>
                        <default>0</default>
                    </config>
                </TCEforms>
            </allow_unstable>
        </el>
    </ROOT>
</T3DataStructure>
